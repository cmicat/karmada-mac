<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.github.karmada-io.karmada-etcd</string>
    <key>ProgramArguments</key>
    <array>
        <string>{{HOME}}/bin/etcd</string>
        <string>--data-dir={{HOME}}/.karmada/etcd</string>
    </array>
    <key>RunAtLoad</key>
    <true/>
    <key>KeepAlive</key>
    <true/>
    <key>StandardErrorPath</key>
    <string>{{HOME}}/.karmada/logs/etcd.error</string>
    <key>StandardOutPath</key>
    <string>{{HOME}}/.karmada/logs/etcd.log</string>
</dict>
</plist>
